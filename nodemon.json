{
  "watch": [
    "docs/_src",
    "_scripts"
  ],
  "ext": "*",
  "ignore": [
    "docs/_src/dist"
  ],
  "delay": ".2",
  "execMap": {
    "js": "npm run build-all",
    "json": "npm run docs:build",
    "md": "npm run docs:build",
    "hbs": "npm run docs:build",
    "styl": "npm run docs:stylus"
  }
}
