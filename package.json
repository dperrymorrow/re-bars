{
  "name": "re-bars",
  "version": "0.0.1",
  "description": "Handlebars templates with re-render / watch blocks",
  "main": "dist/index.js",
  "scripts": {
    "start": "http-server . -c-1",
    "test": "c8 ava --verbose",
    "build": "rollup -c"
  },
  "author": "David Morrow",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "ava": "^3.5.0",
    "babel-plugin-istanbul": "^6.0.0",
    "browser-env": "^3.3.0",
    "c8": "7.1.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "esm": "^3.2.25",
    "handlebars": "^4.7.3",
    "http-server": "^0.12.0",
    "prettier": "^1.19.1",
    "rollup": "^1.31.0",
    "rollup-plugin-copy": "3.3.0",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-gzip": "^2.3.0",
    "rollup-plugin-strip": "^1.2.2",
    "rollup-plugin-terser": "^5.2.0",
    "sinon": "^9.0.0"
  },
  "ava": {
    "require": [
      "esm",
      "./test/setup.js"
    ],
    "files": [
      "./test/**/*.spec.js"
    ],
    "concurrency": 20,
    "failFast": true,
    "verbose": true
  }
